{
  "name": "music_backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest --testEnvironment=node --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "simulate": "node scripts/simulate_room.js",
    "migrate": "node scripts/migrateRoomsToJqbx.js",
    "crawl": "node src/schedule/index.js",
    "crawl:songs": "node src/schedule/index.js songs",
    "crawl:artists": "node src/schedule/index.js artists",
    "crawl:albums": "node src/schedule/index.js albums",
    "crawl:playlists": "node src/schedule/index.js playlists",
    "crawl:mvs": "node src/schedule/index.js mvs",
    "crawl:schedule": "node src/schedule/index.js schedule"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1",
    "node-cron": "^3.0.3",
    "socket.io": "^4.8.1",
    "zingmp3-api-full-v3": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
